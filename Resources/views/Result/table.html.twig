<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="{{ asset('bundles/livevoting/css/bootstrap.css') }}">

    <!-- Optional theme -->
    <link rel="stylesheet" href="css/bootstrap-theme.css">



    <title></title>

</head>
<body>
<div class="container">
    <div class="table-responsive">
        <table class="table table-condensed">

            <tr>
                <th>Place</th>
                <th>Presentation</th>
                <th>Presenter</th>
                <th>Score</th>


            </tr>
            {% for p in presentations['presentations'] %}
            <tr>
                <td>{{ loop.index }}</td>
                <td>{{ p['presentation']['name'] }}</td>
                <td>{{ p['presentation']['entity'].getPresenterName~' '~p['presentation']['entity'].getPresenterSurname }}</td>
                <td>{{ p['score']['average'] }}</td>

            </tr>
            {% endfor %}

        </table>
    </div>
</body>
</html>
